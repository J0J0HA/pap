{
    "name": "dynmap",
    "version": "3.5-beta-3",
    "license": "Apache-2.0",
    "description": "Dynamic web maps for Minecraft servers.",
    "authors": ["mikeprimm"],
    "site": "https://github.com/webbukkit/dynmap",
    "downloads": [
        {
            "type": "url",
            "url": "https://github.com/webbukkit/dynmap/releases/download/{version}/Dynmap-{version}-spigot.jar",
            "filename": "Dynmap.jar"
        }
    ],
    "install": {
        "type": "simple"
    },
    "uninstall": {
        "files": [
            {
                "type": "main",
                "path": "Dynmap.jar"
            },
            {
                "type": "config",
                "path": "Dynmap"
            }
        ]
    }
}
